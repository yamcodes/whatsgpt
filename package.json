{
  "name": "whatsgpt",
  "version": "1.0.0",
  "main": "dist-electron/main/index.js",
  "description": "WhatsApp client for desktop, built with Electron, Vue 3, Vite, and Tailwind CSS.",
  "author": "yamyam263",
  "license": "MIT",
  "private": true,
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue3",
    "vue"
  ],
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
    }
  },
  "scripts": {
    "start": "vite",
    "dev": "yarn devtools & DEVTOOLS=true yarn start",
    "build": "vue-tsc --noEmit && vite build && electron-builder",
    "preview": "vite preview",
    "lint": "eslint --ext .js,.ts,.vue src",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "devtools": "./node_modules/.bin/vue-devtools",
    "generate:index": "ts-node -P tsconfig.script.json scripts/generateComponentIndex.ts",
    "prettier": "prettier --write .",
    "check:prettier": "prettier --check .",
    "hooks": "git config --local core.hooksPath .githooks"
  },
  "dependencies": {
    "@heroicons/vue": "^2.0.18",
    "@testing-library/vue": "^7.0.0",
    "@vitejs/plugin-vue": "^4.1.0",
    "daisyui": "^2.51.5",
    "dotenv": "^16.0.3",
    "theme-change": "^2.5.0",
    "vite": "^4.2.1",
    "vite-plugin-electron": "^0.11.1",
    "vite-plugin-electron-renderer": "^0.13.7",
    "vue": "^3.2.47",
    "vue-router": "^4.0.13",
    "whatsapp-web.js": "^1.19.5"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vitest/ui": "^0.29.2",
    "@vue/devtools": "^6.0.0-beta.21",
    "autoprefixer": "^10.4.14",
    "electron": "^23.2.0",
    "electron-builder": "^23.6.0",
    "eslint": "^8.36.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-vue": "^9.10.0",
    "happy-dom": "^8.9.0",
    "patch-package": "^6.5.1",
    "postcss": "^8.4.21",
    "prettier": "2.8.7",
    "prettier-plugin-tailwindcss": "^0.2.5",
    "sass": "^1.59.3",
    "tailwindcss": "^3.2.7",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2",
    "vite-plugin-windicss": "^1.8.10",
    "vite-tsconfig-paths": "^4.0.7",
    "vitest": "^0.29.2",
    "vue-tsc": "^1.1.7",
    "windicss": "^3.5.6"
  }
}
